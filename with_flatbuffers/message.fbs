table Add {
  a: int;
  b: int;
}

table Subtract {
  a: int;
  b: int;
}

union Command { Add, Subtract }

table AddReturnValue {
  value: int;
}

table SubtractReturnValue {
  value: int;
}

union ReturnValue { AddReturnValue, SubtractReturnValue }

table Request {
  id: int;
  command: Command;
}

table Response {
  id: int;
  return_value: ReturnValue;
}
